{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${PGHOST};Database=${PGDATABASE};Username=${PGUSER};Password=${PGPASSWORD};Port=${PGPORT};"
  },
  "JwtSettings": {
    "SecretKey": "development_key_that_is_at_least_32_characters_long_for_security",
    "Issuer": "PixelSpot.Development",
    "Audience": "PixelSpotClients.Development",
    "ExpiryMinutes": 60
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  }
}
